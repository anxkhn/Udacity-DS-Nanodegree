          "id": 691206,
          "key": "7e01e3fa-28c7-4304-86e2-accff3e13f9d",
          "title": "Using Dummy Tests",
          "semantic_type": "Concept",
          "is_public": true,
          "user_state": {
            "node_key": "7e01e3fa-28c7-4304-86e2-accff3e13f9d",
