        {
          "id": 691204,
          "key": "4ddaf078-35ad-4556-b9dc-b9a671d0d662",
          "title": "Practical Significance",
          "semantic_type": "Concept",
          "is_public": true,
          "user_state": {
            "node_key": "4ddaf078-35ad-4556-b9dc-b9a671d0d662",
            "completed_at": null,
            "last_viewed_at": null,
