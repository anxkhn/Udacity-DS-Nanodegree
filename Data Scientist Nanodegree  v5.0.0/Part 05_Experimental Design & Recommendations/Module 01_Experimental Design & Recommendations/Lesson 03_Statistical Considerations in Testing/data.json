          },
          "resources": null,
          "atoms": [
            {
              "id": 691246,
              "key": "02f6c10e-8083-45a8-b5df-10a9f69a323d",
              "title": "Analyzing Multiple Metrics Pt 1",
              "semantic_type": "VideoAtom",
              "is_public": true,
              "instructor_notes": "",
              "video": {
                "youtube_id": "SNFHYbJvlZU",
