            },
            {
              "id": 708651,
              "key": "5feae0c9-053f-462f-91f4-2c6c3f88ea36",
              "title": null,
              "semantic_type": "WorkspaceAtom",
              "is_public": true,
              "workspace_id": "viewvrgyb1g8sx9",
              "pool_id": "jupyter",
              "view_id": "jupyter-v47xvy4fyg",
              "gpu_capable": false,
              "configuration": {
                "id": "reserved",
                "blueprint": {
                  "conf": {
                    "disk": null,
                    "ports": [],
